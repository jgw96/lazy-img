{
  "components": [
    {
      "tag": "st-img",
      "componentClass": "StImg",
      "componentPath": "components/st-img/st-img.js",
      "styles": {
        "$": {
          "stylePaths": [
            "components/st-img/st-img.scss"
          ]
        }
      },
      "props": [
        {
          "name": "alt",
          "type": "String"
        },
        {
          "name": "fit",
          "type": "Boolean"
        },
        {
          "name": "src",
          "type": "String",
          "watch": [
            "srcChanged"
          ]
        }
      ],
      "states": [
        {
          "name": "loadSrc"
        }
      ],
      "hostElement": {
        "name": "el"
      },
      "shadow": true
    }
  ],
  "bundles": [
    {
      "components": [
        "st-img"
      ]
    }
  ],
  "compiler": {
    "name": "@stencil/core",
    "version": "0.1.4",
    "typescriptVersion": "2.6.2"
  }
}